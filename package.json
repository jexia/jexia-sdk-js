{
  "name": "jexia-sdk-js",
  "version": "0.0.0-development",
  "description": "Jexia Javascript SDK",
  "main": "index.js",
  "module": "./_esm5/index.js",
  "es2015": "./_esm2015/index.js",
  "typings": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "cleanup": "run-p cleanup:*",
    "cleanup:dist": "shx rm -rf dist",
    "cleanup:docs": "shx rm -rf docs",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "tslint \"src/**/*.ts\" \"spec/**/*.ts\"",
    "test": "jest",
    "test:ci": "jest --no-cache --ci --testResultsProcessor=\"jest-junit\"",
    "tdd": "jest --no-cache --watch",
    "tsnode": "ts-node --project tsconfig.examples.json -r tsconfig-paths/register",
    "preexample": "npm run build:main",
    "example": "node extra_docs/examples/javascript/node/node-example.js",
    "predocs": "npm run cleanup:docs",
    "docs": "compodoc src/api -p tsconfig.json --disableSourceCode --disablePrivate --disableProtected --disableInternal --name \"Jexia Javascript SDK\" --output docs --includes extra_docs --includesName Guides -a extra_docs/assets",
    "docs:deploy": "npm run tsnode -- build/api-docs-publish",
    "build": "npm-run-all -l cleanup:dist -p build:*",
    "build:main": "tsc -p ./build/tsconfig.main.json",
    "build:module": "tsc -p ./build/tsconfig.module.json",
    "build:es2015": "tsc -p ./build/tsconfig.es2015.json",
    "build:bundle": "webpack --config build/webpack.config.js",
    "build:package": "shx mkdir -p ./dist && shx cp package.json ./dist/package.json",
    "build:readme": "shx mkdir -p ./dist && shx cp README.md ./dist/README.md",
    "semantic-release": "semantic-release"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.spec).ts?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/spec/setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "src/config/",
      "src/browser.ts",
      "src/node.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 99,
        "lines": 99,
        "statements": 99
      }
    },
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jexia/jexia-sdk-js.git"
  },
  "author": "jexia.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jexia/jexia-sdk-js/issues"
  },
  "homepage": "https://github.com/jexia/jexia-sdk-js#readme",
  "dependencies": {
    "injection-js": "^2.2.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@compodoc/compodoc": "^1.1.2",
    "@semantic-release/changelog": "^2.0.2",
    "@semantic-release/git": "^5.0.0",
    "@semantic-release/github": "^4.2.17",
    "@semantic-release/npm": "^3.3.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.5.2",
    "@types/node-fetch": "1.6.7",
    "@types/ws": "^3.0.2",
    "commitizen": "^2.9.6",
    "condition-circle": "2.0.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-junit": "^3.7.0",
    "node-fetch": "^2.1.1",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "opn-cli": "^3.1.0",
    "semantic-release": "^15.5.1",
    "shx": "^0.2.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.3",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "ws": "^3.1.0"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "condition-circle",
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      },
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      },
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github",
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
